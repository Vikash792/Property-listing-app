<% layout("/layout/boilerplate.ejs") %>
<body>
   
    
         <h3>All Listings</h3>
         <!-- <form method="GET" action="/alllisting/new">
                  <button>Create New Listing</button>
         </form> -->
<div class="row row-cols-lg-3 row-cols-md-2 row-cols-sm-1 ">
 <% for(listing of alllisting){ %>
  <a href="/alllisting/<%=listing._id%>" class="listing ">
      <div class="card col listing-card" >
        <img class="card-img-top" src="<%=listing.image.url%>" alt="Card image cap">
        <div class="card-img-overlay">
            <h5 class="card-title"></h5>
            <p class="card-text"></p>
            <p class="card-text"></p>
        </div>

        <div class="card-body">
         <p class="card-text">
          <b><%=listing.title%></b>        <br>
          
                &#x20B9;  <%=listing.price.toLocaleString("en-IN")%> /night
              
        </p>
        </div>
      </div>
  </a>
  <%}%>
</div>
  
</body>
